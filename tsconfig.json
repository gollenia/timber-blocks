{
	"compilerOptions": {
		"outDir": "./assets/public/",
		"jsx": "react-jsx",
		"target": "ES2021", // Set to ES5 for older browser support.
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"allowJs": true,
		"moduleResolution": "node",
		"resolveJsonModule": true
	},
	"include": [ "src" ]
}
